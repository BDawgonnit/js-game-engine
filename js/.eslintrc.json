{
	"env": {
		"browser": true,
		"amd": true,
		"es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
		"eqeqeq": "error",
		"no-var": "error",
		"semi": [
			"error",
			"always"
		],
		"no-warning-comments": "warn",
		"no-else-return": "warn",
		"no-alert": "warn",
		"no-trailing-spaces": [
			"warn",
			{ "skipBlankLines": true }
		],
		"indent": [
			"warn",
     	"tab",
			{ "SwitchCase": 1 }
		],
		"quotes": [
			"warn",
			"single"
		]
	}
}
